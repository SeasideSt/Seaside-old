hooks
style
	^ '#lightbox {
	background: url(' , (SUAllTestLibrary / #demoGif) greaseString , ') repeat white;
	border: 1px solid #e8a400;
	padding: 1em;
	width: 640px;
}
#browser h1 {
	margin: 0;
	padding: 0;
	font-size: 15px;
	text-align: center;
	padding-bottom: 1em;
}
#browser select {
	width: 100%;
}
#browser .selectors {
	margin-bottom: 1em;
}
#browser #source {
	padding: 1em;
	margin-bottom: 1em;
	border: 1px solid #dddddd;
	background-color: #f5f5f5;
	font: 12px Lucida Sans Typewriter, Courier, monospace;
}
#browser .close {
	text-align: center;
}'